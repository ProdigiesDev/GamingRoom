<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="779.0" prefWidth="1070.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.gamingroom.gui.StatistiqueController">
   <children>
      <BarChart fx:id="barchart" layoutX="66.0" layoutY="131.0" prefHeight="452.0" prefWidth="685.0">
        <xAxis>
          <CategoryAxis fx:id="XAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="Yaxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="load" layoutX="797.0" layoutY="616.0" mnemonicParsing="false" onAction="#LoadStat" prefHeight="31.0" prefWidth="160.0" text="load" textFill="#5b0a50">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label layoutX="300.0" layoutY="42.0" text="Statistique des produits en fonction de leurs prix ">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Button layoutX="970.0" layoutY="727.0" mnemonicParsing="false" onAction="#Back" text="BACK" textFill="#5b0a50">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
