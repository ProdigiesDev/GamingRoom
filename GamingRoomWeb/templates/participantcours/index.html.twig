    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for participantcour in participantcours %}
            <tr>
                <td>{{ participantcour.id }}</td>
                <td>
